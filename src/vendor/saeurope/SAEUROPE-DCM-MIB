	SAEUROPE-DCM-MIB DEFINITIONS ::= BEGIN
 
		IMPORTS
			tsProcessorsMIBGroups, tsProcessors			
				FROM SAEUROPE-ROOT-MIB			
			OBJECT-GROUP, MODULE-COMPLIANCE			
				FROM SNMPv2-CONF			
			IpAddress, Integer32, Unsigned32, Counter64, OBJECT-TYPE, 
			MODULE-IDENTITY			
				FROM SNMPv2-SMI			
			RowStatus, TruthValue, DisplayString			
				FROM SNMPv2-TC;
	
	
		saEuropeDcmModule MODULE-IDENTITY 
			LAST-UPDATED "201206141300Z"		-- June 14, 2012 at 13:00 GMT
			ORGANIZATION 
				"Synamedia"
			CONTACT-INFO 
				"https://www.synamedia.com/video-professional-services/"
			DESCRIPTION 
				"MIB for the DCM device.
				In all the tables in this MIB, UNDO is not guaranteed to work on a row destruction.
				i.e. if you destroy a row and simultaneously do something else that fails, it is not guaranteed
				that the row will be restored, and if it is restored, it might have changed."
			REVISION "200602161440Z"		-- February 16, 2006 at 14:40 GMT
			DESCRIPTION 
				"V01.00.00
				Initial version of the DCM MIB."
			REVISION "200610111458Z"		-- October 11, 2006 at 14:58 GMT
			DESCRIPTION 
				"V01.00.01
				Added dcmConfigInputTsTable, such that (board, port, UDP, IP) can
				be converted into an input TS index with a single SNMP GETNEXT."
			REVISION "200612041300Z"		-- December 04, 2006 at 13:00 GMT
			DESCRIPTION 
				"V01.00.02
				Added dcmTrGroupPortNumber to the index of the dcmTrGroupTable."
			REVISION "200612211300Z"		-- December 21, 2006 at 13:00 GMT
			DESCRIPTION 
				"V01.00.03
				dcmTrGroupType is now read-create
				ColumnNumbers now follow each other (various tables)
				Fixed range of transrater group id's, Ts index"
			REVISION "200701051300Z"		-- January 5, 2007 at 13:00 GMT
			DESCRIPTION 
				"V01.00.04
				various changes to dpi table, improved documentation"
			REVISION "200705241300Z"		-- May 24, 2007 at 13:00 GMT
			DESCRIPTION 
				"V01.00.05
				support two new DPI modes"
			REVISION "200711131300Z"		-- November 13, 2007 at 13:00 GMT
			DESCRIPTION 
				"V01.00.06, DCM 5.0 release
				support for AutoPass feature"
			REVISION "200801141300Z"		-- January 14, 2008 at 13:00 GMT
			DESCRIPTION 
				"V01.00.07, DCM 5.1 release
				support for Fec Encoding on GbE OutputTs"
			REVISION "200901131300Z"		-- January 13, 2009 at 13:00 GMT
			DESCRIPTION 
				"V01.00.08, DCM 6.9 release
				support for Transparent loopthrough and ASI input bandwidth limitations"
			REVISION "200910131300Z"		-- October 13, 2009 at 13:00 GMT
			DESCRIPTION 
				"V01.00.09, DCM 8.0 release
				support for triggering service backup (per board or per TS) and device backup"
			REVISION "201001151300Z"		-- January 15, 2010 at 13:00 GMT
			DESCRIPTION 
				"V01.00.09, DCM 8.1 release
				support for service backup and ts backup"
			REVISION "201105271300Z"		-- May 27, 2011 at 13:00 GMT
			DESCRIPTION 
				"V01.00.10, DCM 9.0 release
				support for IPGW"
			REVISION "201203021300Z"		-- March 3, 2012 at 13:00 GMT
			DESCRIPTION 
				"V01.00.11, DCM 10.10 release
				support for 4 IPGW boards"
			REVISION "201206141300Z"		-- June 14, 2012 at 13:00 GMT
			DESCRIPTION 
				"V01.00.12, DCM 10.20 release
				Extended support for service backup (MFP,TC,DVBS2)"
			REVISION "201307121300Z"		-- July 12, 2013 at 13:00 GMT
			DESCRIPTION 
				"V01.00.13, DCM 13.0 release
				support for GbE port mode"
			::= { tsProcessors 1 }

		
	
	
--
-- Node definitions
--
	
		dcmMibGroups OBJECT IDENTIFIER ::= { tsProcessorsMIBGroups 1 }

		
--  ************************************************************************
-- 
-- Module compliance
-- 
-- ************************************************************************
		saEuropeDcmModuleCompliance MODULE-COMPLIANCE
			STATUS current
			DESCRIPTION 
				"The compliance statement for entities which
				implement the SA Europe DCM MIB"
			MODULE -- this module
				MANDATORY-GROUPS { dcmMandatoryGroup }
			::= { dcmMibGroups 1 }

		
		dcmMandatoryGroup OBJECT-GROUP
			OBJECTS { dcmInputTsBoard, dcmInputTsPort, dcmInputTsUdpPort, dcmInputTsIpAddress, dcmInputTsSrcIpAddress, 
				dcmCfgInputTsIndex, dcmCfgInputTsSrcIpAddress, dcmCfgInputTsState, dcmOutputTsBoard, dcmOutputTsPort, 
				dcmOutputTsUdpPort, dcmOutputTsIpAddress, dcmOutputTsTransportStreamId, dcmOutputTsOriginalNetworkId, dcmOutputTsStandard, 
				dcmOutputTsActive, dcmOutputTsTransportStreamBitRate, dcmCfgOutputTsIndex, dcmCfgOutputTsTransportStreamId, dcmCfgOutputTsOriginalNetworkId, 
				dcmCfgOutputTsStandard, dcmCfgOutputTsActive, dcmCfgOutputTsTransportStreamBitRate, dcmCfgOutputTsRowStatus, dcmTrGroupBitRate, 
				dcmTrGroupType, dcmTrGroupRowStatus, dcmCfgSrvForceOutputServiceID, dcmCfgSrvOutputServiceID, dcmCfgSrvForceOutputServiceName, 
				dcmCfgSrvOutputServiceName, dcmCfgSrvRowStatus, dcmCfgSrvState }
			STATUS current
			DESCRIPTION 
				"Mandatory objects for all devices implementing the
				SA Europe DCM MIB."
			::= { dcmMibGroups 2 }

		
		dcmOptionalGroup OBJECT-GROUP
			OBJECTS { dcmCfgInputTsRowStatus, dcmTrTsGroupId, dcmTrTsRowStatus, dcmTrSrvGroupId, dcmTrSrvTypeForService, 
				dcmTrSrvLowerLimitServiceBitRate, dcmTrSrvUpperLimitServiceBitRate, dcmTrSrvPriority, dcmTrSrvRowStatus, dcmDpiPrimaryChannelName, 
				dcmDpiPrimaryChannelMode, dcmDpiInputTsIndex, dcmDpiInputServiceId, dcmDpiManualTrigger, dcmDpiChannelStatus, 
				dcmDpiRowStatus }
			STATUS current
			DESCRIPTION 
				"Optional objects in the SA Europe DCM MIB."
			::= { dcmMibGroups 3 }

		
		dcmIpgwGroup OBJECT-GROUP
			OBJECTS { dcmIpgwSdiInConfigMaxAllowedVideoFormat, dcmIpgwSdiInConfigUserName, dcmIpgwSdiInFormatFramerate, dcmIpgwSdiInFormatVideoformat, dcmIpgwSdiInFormatAspectRatio, 
				dcmIpgwSdiInFormatNbrOfLinesPerFrame, dcmIpgwSdiInFormatFrameBuildUp, dcmIpgwSdiInStatisticsReset, dcmIpgwSdiInStatisticsCrcErrors1Min, dcmIpgwSdiInStatisticsCrcErrors1Hour, 
				dcmIpgwSdiInStatisticsCrcErrors1Day, dcmIpgwRtpInTSMaxAllowedVideoFormat, dcmIpgwRtpInTSUsername, dcmIpgwRtpInTSIpAddress, dcmIpgwRtpInTSUdpPort, 
				dcmIpgwRtpInTSDejitter, dcmIpgwRtpInTSVideoOffset, dcmIpgwRtpInTSRowStatus, dcmIpgwRtpInJp2kEnableState, dcmIpgwRtpInStatisticsReset, 
				dcmIpgwRtpInStatisticsValidPackets, dcmIpgwRtpInStatisticsMissingPackets, dcmIpgwRtpInStatisticsFixedPackets, dcmIpgwRtpInStatisticsDuplicatePackets, dcmIpgwRtpInStatisticsReorderedPackets, 
				dcmIpgwRtpInStatisticsCrcReset, dcmIpgwRtpInStatisticsCrcErrors1min, dcmIpgwRtpInStatisticsCrcErrors1Hour, dcmIpgwRtpInStatisticsCrcErrors1Day, dcmIpgwRtpInFecConfigMode, 
				dcmIpgwRtpInFecConfigColumnIpAddress, dcmIpgwRtpInFecConfigColumnUdpPort, dcmIpgwRtpInFecConfigRowIpAddress, dcmIpgwRtpInFecConfigRowUdpPort, dcmIpgwRtpInFecStatusColumnPresent, 
				dcmIpgwRtpInFecStatusRowPresent, dcmIpgwRtpInFecStatusFecColumnDepth, dcmIpgwRtpInFecStatusFecRowLength, dcmIpgwRtpInFecStatusColumnOverhead, dcmIpgwRtpInFecStatusRowOverhead, 
				dcmIpgwRtpInFecStatusTotalOverhead, dcmIpgwRtpInFecStatusLatency, dcmIpgwRtpInHitlessMode, dcmIpgwRtpInHitlessWindowPackets, dcmIpgwRtpInHitlessDelay, 
				dcmIpgwRtpOutTSActive, dcmIpgwRtpOutTSGbePortType, dcmIpgwRtpOutTSRowStatus, dcmIpgwRtpOutJp2kEnableState, dcmIpgwRtpOutJp2kConfigVideoRate, 
				dcmIpgwRtpOutJp2kConfigMaxStreamBitRate, dcmIpgwRtpOutJp2kConfigChromaWeight, dcmIpgwRtpOutJp2kConfigVitcEnable, dcmIpgwRtpOutJp2kConfigViiEnable, dcmIpgwRtpOutJp2kConfigAfdEnable, 
				dcmIpgwRtpOutJp2kConfigTeletextBEnable, dcmIpgwRtpOutJp2kConfigCCEnable, dcmIpgwRtpOutJp2kConfigEncapsulationType, dcmIpgwRtpOutAdvancedVlanEnable, dcmIpgwRtpOutAdvancedVlanId, dcmIpgwRtpOutAdvancedVlanPriority, 
				dcmIpgwRtpOutAdvancedTtl, dcmIpgwRtpOutAdvancedTos, dcmIpgwRtpOutAdvancedSourceIpAddress, dcmIpgwRtpOutAdvancedSourceUdpPort, dcmIpgwRtpOutFecMode, 
				dcmIpgwRtpOutFecSendingArrangement, dcmIpgwRtpOutFecRowLength, dcmIpgwRtpOutFecColumnDepth, dcmIpgwRtpOutFecOverheadColumn, dcmIpgwRtpOutFecOverheadRow, 
				dcmIpgwSdiOutTSSrcRef, dcmIpgwSdiOutTSSrcPort, dcmIpgwSdiOutTSSrcBoard, dcmIpgwRtpOutFecDestinationColIpAddressA, dcmIpgwRtpOutFecDestinationColUdpPortA, 
				dcmIpgwRtpOutFecDestinationRowIpAddressA, dcmIpgwRtpOutFecDestinationRowUdpPortA, dcmIpgwRtpOutFecDestinationColIpAddressB, dcmIpgwRtpOutFecDestinationColUdpPortB, dcmIpgwRtpOutFecDestinationRowIpAddressB, 
				dcmIpgwRtpOutFecDestinationRowUdpPortB, dcmIpgwRtpOutTSDestinationIpAddressA, dcmIpgwRtpOutTSDestinationUdpPortA, dcmIpgwRtpOutTSDestinationIpAddressB, dcmIpgwRtpOutTSDestinationUdpPortB, 
				dcmIpgwRtpOutFecOverheadTotal, dcmIpgwSdiOutTSActive, dcmIpgwRtpOutTSSrcBoard, dcmIpgwRtpOutTSSrcPort, dcmIpgwRtpOutAdvancedUseSourceAddress, 
				dcmIpgwRtpOutJp2kConfigTrans525F2Line, dcmIpgwRtpOutJp2kConfigTrans525F1Line, dcmIpgwRtpOutJp2kConfigTrans625F2Line, dcmIpgwRtpOutJp2kConfigTrans625F1Line, dcmIpgwRtpOutJp2kConfigVPSEnable, 
				dcmIpgwRtpOutTSSrcRef, dcmIpgwRtpInStatisticsOutOfRangePackets, dcmIpgwRtpOutJp2kAudioPair2Enable, dcmIpgwRtpOutJp2kAudioPair1Enable, dcmIpgwRtpOutJp2kAudioPair1Present, 
				dcmIpgwRtpOutJp2kAudioPair6Present, dcmIpgwRtpOutJp2kAudioPair7Enable, dcmIpgwRtpOutJp2kAudioPair7Present, dcmIpgwRtpOutJp2kAudioPair8Enable, dcmIpgwRtpOutJp2kAudioPair8Present, 
				dcmIpgwRtpOutJp2kAudioPair4Enable, dcmIpgwRtpOutJp2kAudioPair4Present, dcmIpgwRtpOutJp2kAudioPair5Enable, dcmIpgwRtpOutJp2kAudioPair5Present, dcmIpgwAsiInConfigMaxAllowedVideoFormat, 
				dcmIpgwAsiInConfigUserName, dcmIpgwAsiInConfigVideoOffset, dcmIpgwAsiInFormatFramerate, dcmIpgwAsiInFormatVideoformat, dcmIpgwAsiInFormatAspectRatio, 
				dcmIpgwAsiInFormatNbrOfLinesPerFrame, dcmIpgwAsiInFormatFrameBuildUp, dcmIpgwAsiInStatisticsReset, dcmIpgwAsiInStatisticsCrcErrors1Min, dcmIpgwAsiInStatisticsCrcErrors1Hour, 
				dcmIpgwAsiInStatisticsCrcErrors1Day, dcmIpgwAsiOutTSActive, dcmIpgwASIOutJp2kConfigVideoRate, dcmIpgwASIOutJp2kConfigMaxStreamBitRate, dcmIpgwASIOutJp2kConfigChromaWeight, 
				dcmIpgwASIOutJp2kConfigVitcEnable, dcmIpgwASIOutJp2kConfigViiEnable, dcmIpgwASIOutJp2kConfigAfdEnable, dcmIpgwASIOutJp2kConfigTeletextBEnable, dcmIpgwASIOutJp2kConfigCCEnable, 
				dcmIpgwASIOutJp2kConfigVPSEnable, dcmIpgwASIOutJp2kConfigTrans625F1Line, dcmIpgwASIOutJp2kConfigTrans625F2Line, dcmIpgwASIOutJp2kConfigTrans525F1Line, dcmIpgwASIOutJp2kConfigTrans525F2Line, 
				dcmIpgwASIOutJp2kAudioPair1Enable, dcmIpgwASIOutJp2kAudioPair1Present, dcmIpgwASIOutJp2kAudioPair2Enable, dcmIpgwASIOutJp2kAudioPair2Present, dcmIpgwASIOutJp2kAudioPair3Enable, 
				dcmIpgwASIOutJp2kAudioPair3Present, dcmIpgwASIOutJp2kAudioPair4Enable, dcmIpgwASIOutJp2kAudioPair4Present, dcmIpgwASIOutJp2kAudioPair5Enable, dcmIpgwASIOutJp2kAudioPair5Present, 
				dcmIpgwASIOutJp2kAudioPair6Enable, dcmIpgwASIOutJp2kAudioPair6Present, dcmIpgwASIOutJp2kAudioPair7Enable, dcmIpgwASIOutJp2kAudioPair7Present, dcmIpgwASIOutJp2kAudioPair8Enable, 
				dcmIpgwASIOutJp2kAudioPair8Present, dcmIpgwSystemGenlockEnableState, dcmIpgwRtpInFormatFramerate, dcmIpgwRtpInFormatVideoformat, dcmIpgwRtpInFormatAspectRatio, 
				dcmIpgwRtpInFormatNbrOfLinesPerFrame, dcmIpgwRtpInFormatFrameBuildUp, dcmIpgwRtpOutJp2kAudioPair6Enable, dcmIpgwRtpOutJp2kAudioPair2Present, dcmIpgwRtpOutJp2kAudioPair3Enable, 
				dcmIpgwRtpOutJp2kAudioPair3Present }
			STATUS current
			DESCRIPTION 
				"Description."
			::= { dcmMibGroups 4 }

		
		dcmMIB OBJECT IDENTIFIER ::= { tsProcessors 3 }

		
		dcmSettings OBJECT IDENTIFIER ::= { dcmMIB 1 }

		
		dcmMuxSettings OBJECT IDENTIFIER ::= { dcmSettings 1 }

		
--   ***********************************************************************
-- 
-- DCM Settings
-- 
-- ***********************************************************************
-- input transport stream identification
		dcmInputTransportStreamTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmInputTransportStreamEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the transport streams present at the input
				ports of the DCM."
			::= { dcmMuxSettings 1 }

		
		dcmInputTransportStreamEntry OBJECT-TYPE
			SYNTAX DcmInputTransportStreamEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of the dcmInputTransportStreamTable."
			INDEX { dcmInputTsIndex }
			::= { dcmInputTransportStreamTable 1 }

		
		DcmInputTransportStreamEntry ::=
			SEQUENCE { 
				dcmInputTsIndex
					Integer32,
				dcmInputTsBoard
					Integer32,
				dcmInputTsPort
					Integer32,
				dcmInputTsUdpPort
					Integer32,
				dcmInputTsIpAddress
					IpAddress,
				dcmInputTsSrcIpAddress
					IpAddress
			 }

		dcmInputTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the input transport stream."
			::= { dcmInputTransportStreamEntry 1 }

		
		dcmInputTsBoard OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identification of the board where the service is coming in."
			::= { dcmInputTransportStreamEntry 2 }

		
		dcmInputTsPort OBJECT-TYPE
			SYNTAX Integer32 (1..10)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identification of the port where the service is coming in.
				For ASI boards port numbers range from 1..10
				For GbE boards port numbers can range from 1..4.
				Only 1 and 3 can be used (2 and 4 are backup ports)"
			::= { dcmInputTransportStreamEntry 3 }

		
		dcmInputTsUdpPort OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For an input TS on a GbE board, this is the identification of the
				UDP port. For an input TS on an ASI board this object will be 0."
			::= { dcmInputTransportStreamEntry 4 }

		
		dcmInputTsIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For an input transport stream on a GbE board, this is the destination
				(multicast) IP address of the stream containing the input
				service. For an input TS on an ASI board this object will be 0.0.0.0."
			::= { dcmInputTransportStreamEntry 5 }

		
		dcmInputTsSrcIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For an input transport stream on a GbE board, this is the
				source IP address of the stream containing the input
				service. If the source IP address of a GbE input transport
				stream is not known, or when the input transport stream
				arrives on an ASI board, then the IP address 0.0.0.0 will
				be returned."
			::= { dcmInputTransportStreamEntry 6 }

		
--  input transport stream configuration
		dcmConfigInputTsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmConfigInputTsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"This table contains the transport streams present at the input
				ports of the DCM, and allows preconfiguration of input streams."
			::= { dcmMuxSettings 2 }

		
		dcmConfigInputTsEntry OBJECT-TYPE
			SYNTAX DcmConfigInputTsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of the dcmConfigInputTsTable."
			INDEX { dcmCfgInputTsBoard, dcmCfgInputTsPort, dcmCfgInputTsUdpPort, dcmCfgInputTsIpAddress }
			::= { dcmConfigInputTsTable 1 }

		
		DcmConfigInputTsEntry ::=
			SEQUENCE { 
				dcmCfgInputTsBoard
					Integer32,
				dcmCfgInputTsPort
					Integer32,
				dcmCfgInputTsUdpPort
					Integer32,
				dcmCfgInputTsIpAddress
					IpAddress,
				dcmCfgInputTsIndex
					Integer32,
				dcmCfgInputTsSrcIpAddress
					IpAddress,
				dcmCfgInputTsRowStatus
					RowStatus,
				dcmCfgInputTsState
					INTEGER
			 }

		dcmCfgInputTsBoard OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the board where the service is coming in."
			::= { dcmConfigInputTsEntry 1 }

		
		dcmCfgInputTsPort OBJECT-TYPE
			SYNTAX Integer32 (1..10)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the port where the service is coming in.
				For ASI boards port numbers range from 1..10
				For GbE boards port numbers can range from 1..4.
				Only 1 and 3 can be used (2 and 4 are backup ports)"
			::= { dcmConfigInputTsEntry 2 }

		
		dcmCfgInputTsUdpPort OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"For an input TS on a GbE board, this is the identification of the
				UDP port. For an input TS on an ASI board, specify 0."
			::= { dcmConfigInputTsEntry 3 }

		
		dcmCfgInputTsIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"For an input transport stream on a GbE board, this is the destination
				(multicast) IP address of the stream containing the input
				service. For an input TS on an ASI board, specify 0.0.0.0."
			::= { dcmConfigInputTsEntry 4 }

		
		dcmCfgInputTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identification of the input transport stream."
			::= { dcmConfigInputTsEntry 5 }

		
		dcmCfgInputTsSrcIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"For an input transport stream on a GbE board, this is the
				source IP address of the stream containing the input
				service(s). If the source IP address of a GbE input transport
				stream is not known, or when the input transport stream
				arrives on an ASI board, then the IP address 0.0.0.0 will
				be returned."
			::= { dcmConfigInputTsEntry 6 }

		
		dcmCfgInputTsRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Rowstatus column that allows to preconfigure an input transport stream.
				Can only support one-shot creation and deletion (createAndGo(4)/destroy(6))
				SNMP UNDO of a destroy might fail.
				"
			::= { dcmConfigInputTsEntry 7 }

		
		dcmCfgInputTsState OBJECT-TYPE
			SYNTAX INTEGER
				{
				present(1),
				notPresent(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"State of the input transport stream, based on TS-Loss alarm."
			::= { dcmConfigInputTsEntry 8 }

		
--  output transportstream identification
		dcmOutputTransportStreamTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmOutputTransportStreamEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with the output transport streams. This table contains both
				actual and preconfigured transport streams."
			::= { dcmMuxSettings 3 }

		
		dcmOutputTransportStreamEntry OBJECT-TYPE
			SYNTAX DcmOutputTransportStreamEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of the dcmOutputTransportStreamTable."
			INDEX { dcmOutputTsIndex }
			::= { dcmOutputTransportStreamTable 1 }

		
		DcmOutputTransportStreamEntry ::=
			SEQUENCE { 
				dcmOutputTsIndex
					Integer32,
				dcmOutputTsBoard
					Integer32,
				dcmOutputTsPort
					Integer32,
				dcmOutputTsUdpPort
					Integer32,
				dcmOutputTsIpAddress
					IpAddress,
				dcmOutputTsTransportStreamId
					Integer32,
				dcmOutputTsOriginalNetworkId
					Integer32,
				dcmOutputTsStandard
					INTEGER,
				dcmOutputTsActive
					INTEGER,
				dcmOutputTsTransportStreamBitRate
					Integer32
			 }

		dcmOutputTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the outgoing transport stream."
			::= { dcmOutputTransportStreamEntry 1 }

		
		dcmOutputTsBoard OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identification of the board where output transport stream is
				located."
			::= { dcmOutputTransportStreamEntry 2 }

		
		dcmOutputTsPort OBJECT-TYPE
			SYNTAX Integer32 (1..10)
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"Identification of the outgoing port."
			::= { dcmOutputTransportStreamEntry 3 }

		
		dcmOutputTsUdpPort OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Destination UDP port in case of a GbE stream. This object will be 0
				for ASI streams."
			::= { dcmOutputTransportStreamEntry 4 }

		
		dcmOutputTsIpAddress OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Destination IP address in case of a GbE stream. This object will be
				0.0.0.0 for ASI streams."
			::= { dcmOutputTransportStreamEntry 5 }

		
		dcmOutputTsTransportStreamId OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"For GbE transport streams, the transport stream ID (TSID) of the
				output transport stream. For ASI transport streams there is no
				instance of this object."
			::= { dcmOutputTransportStreamEntry 6 }

		
		dcmOutputTsOriginalNetworkId OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"For GbE transport streams, the Original Network ID (ONID) of the
				output transport stream. For ASI transport streams there is no
				instance of this object."
			::= { dcmOutputTransportStreamEntry 7 }

		
		dcmOutputTsStandard OBJECT-TYPE
			SYNTAX INTEGER
				{
				transparent(1),
				forceUpLink(5),
				forceVideo(6),
				forceAudio3(7)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Transport stream setting - PMT generation standard.
				Values between transparent and forceUpLink result in undefined behaviour."
			::= { dcmOutputTransportStreamEntry 8 }

		
		dcmOutputTsActive OBJECT-TYPE
			SYNTAX INTEGER
				{
				active(1),
				notActive(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Transport stream active or not."
			::= { dcmOutputTransportStreamEntry 9 }

		
