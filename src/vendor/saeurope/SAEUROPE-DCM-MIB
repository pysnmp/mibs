			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The Rtp Output reference."
			::= { dcmIpgwRtpOutTSEntry 3 }

		
		dcmIpgwRtpOutTSSrcBoard OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The Board from where the stream is routed from.
				Must be the same dcmIpgwRtpOutTSSrcBoard.
				
				If Board, Port, Ref is 0,0,0 it indicates that there is no Stream routed to the RTP Output port.
				To delete a Route, write 0,0,0 to Board, Port, Ref."
			::= { dcmIpgwRtpOutTSEntry 4 }

		
		dcmIpgwRtpOutTSSrcPort OBJECT-TYPE
			SYNTAX Integer32 (0 | 2..7 | 101..106)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"The Port from where the stream is routed from.
				2-7 if routing a SDI input stream.
				If Board, Port, Ref is 0,0,0 it indicates that there is no Stream routed to the RTP Output port.
				To delete a Route, write 0,0,0 to Board, Port, Ref."
			::= { dcmIpgwRtpOutTSEntry 5 }

		
		dcmIpgwRtpOutTSSrcRef OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"If routed from the Gbe port, the Ref must also be given.
				The Ref is listed in dcmIpgwRtpInTSRef. 
				Not used if routed from a SDI port.
				
				If Board, Port, Ref is 0,0,0 it indicates that there is no Stream routed to the RTP Output port.
				To delete a Route, write 0,0,0 to Board, Port, Ref."
			::= { dcmIpgwRtpOutTSEntry 6 }

		
		dcmIpgwRtpOutTSActive OBJECT-TYPE
			SYNTAX INTEGER
				{
				eActive(1),
				eNotActive(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Activate or Pause the output of the RTP Stream."
			::= { dcmIpgwRtpOutTSEntry 7 }

		
		dcmIpgwRtpOutTSGbePortType OBJECT-TYPE
			SYNTAX INTEGER
				{
				ePairType(1),
				eIndividualType(2)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Pair: All configuration values like: TTL, TOS, VLAN, srcAddr (source socket)
				  and destination socket are the same for the two GbE ports. Values configured 
				  for GbE0 are also used for GbE1.
				Individual: All configuration values as mentioned above are individual 
				  for for each GbE port.
				"
			::= { dcmIpgwRtpOutTSEntry 8 }

		
		dcmIpgwRtpOutTSDestinationIpAddressA OBJECT-TYPE
			SYNTAX IpAddress
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Destination IP Address.
				If dcmIpgwRtpOutTSGbePortType is set ePairType this
				address is used on both the A and B port."
			::= { dcmIpgwRtpOutTSEntry 9 }

		
