				"
			::= { dcmServiceBackupByTsEntry 2 }

		
--  Input service backup settings
		dcmServiceBackupSettingsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmServiceBackupSettingsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with settings for service backup."
			::= { dcmServiceBackup 3 }

		
		dcmServiceBackupSettingsEntry OBJECT-TYPE
			SYNTAX DcmServiceBackupSettingsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of dcmServiceBackupSettingsTable"
			INDEX { dcmSrvBuSettingsTsIndex, dcmSrvBuSettingsServiceID }
			::= { dcmServiceBackupSettingsTable 1 }

		
		DcmServiceBackupSettingsEntry ::=
			SEQUENCE { 
				dcmSrvBuSettingsTsIndex
					Integer32,
				dcmSrvBuSettingsServiceID
					Integer32,
				dcmSrvBuSettingsBehavior
					INTEGER,
				dcmSrvBuSettingsToBackupDelay
					Integer32,
				dcmSrvBuSettingsToMainDelay
					Integer32,
				dcmSrvBuSettingsState
					INTEGER
			 }

		dcmSrvBuSettingsTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the transport stream of the output service."
			::= { dcmServiceBackupSettingsEntry 1 }

		
		dcmSrvBuSettingsServiceID OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The service ID (PAT programme number) of the output service."
			::= { dcmServiceBackupSettingsEntry 2 }

		
		dcmSrvBuSettingsBehavior OBJECT-TYPE
			SYNTAX INTEGER
				{
				nonRevertive(1),
				revertive(2),
				floating(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Behavior of the backup:
				- non revertive: when switched to lower priority backup service, 
				                 manual switch is needed to return to higher priority input service.
				- revertive: when switched to lower priority backup service, 
				             return to higher priority input service when one is present.
				- floating: when switched to lower priority backup service,
				            return to higher priority input service when one is present and 
				            the current active input service is not present."
			::= { dcmServiceBackupSettingsEntry 3 }

		
		dcmSrvBuSettingsToBackupDelay OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The delay in seconds before a lower priority backup service is chosen when
				the active service is not present anymore."
			::= { dcmServiceBackupSettingsEntry 4 }

		
		dcmSrvBuSettingsToMainDelay OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The delay in seconds before a higher priority backup service is chosen when:
				- revertive: a higher priority input service becomes present 
				- floating: the active service is not present anymore 
				            and a higher priority input service is present."
			::= { dcmServiceBackupSettingsEntry 5 }

		
		dcmSrvBuSettingsState OBJECT-TYPE
			SYNTAX INTEGER
				{
				main(1),
				backup(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Read out the current state and select main 
				(selecting a backup service must be done in de backup service table,
				 setting the value to backup will be ignored)."
			::= { dcmServiceBackupSettingsEntry 6 }

		
--  Input service backup list
		dcmServiceBackupServicesTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmServiceBackupServicesEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with list of backup services, the order of the list defines the priority:
				when the main input service is not present anymore, the first backup service will 
				be active when present.
				Remark: because snmp does not have support for merged in services and alternate
				        services, this table does not support back up for them.
				At the moment of writing, the maximum backup services per output service
				is 1."
			::= { dcmServiceBackup 4 }

		
		dcmServiceBackupServicesEntry OBJECT-TYPE
			SYNTAX DcmServiceBackupServicesEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of dcmServiceBackupSettingsTable"
			INDEX { dcmSrvBuOutputSrvTsIndex, dcmSrvBuOutputSrvServiceID, dcmSrvBuBuSrvTsIndex, dcmSrvBuBuSrvServiceID }
			::= { dcmServiceBackupServicesTable 1 }

		
		DcmServiceBackupServicesEntry ::=
			SEQUENCE { 
				dcmSrvBuOutputSrvTsIndex
					Integer32,
				dcmSrvBuOutputSrvServiceID
					Integer32,
				dcmSrvBuBuSrvTsIndex
					Integer32,
				dcmSrvBuBuSrvServiceID
					Integer32,
				dcmSrvBuBuSrvState
					INTEGER,
				dcmSrvBuRowStatus
					RowStatus
			 }

		dcmSrvBuOutputSrvTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the transport stream of the output service."
			::= { dcmServiceBackupServicesEntry 1 }

		
		dcmSrvBuOutputSrvServiceID OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The service ID (PAT programme number) of the output service."
			::= { dcmServiceBackupServicesEntry 2 }

		
		dcmSrvBuBuSrvTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the transport stream of the backup service."
			::= { dcmServiceBackupServicesEntry 3 }

		
		dcmSrvBuBuSrvServiceID OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The service ID (PAT programme number) of the backup service."
			::= { dcmServiceBackupServicesEntry 4 }

		
		dcmSrvBuBuSrvState OBJECT-TYPE
			SYNTAX INTEGER
				{
				active(1),
				notActive(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Read out the current state and select the backup service by setting the
				value to active (setting the value to notActive will be ignored)."
			::= { dcmServiceBackupServicesEntry 5 }

		
		dcmSrvBuRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Adding a row creates a new backup service for the output service.
				Deleting the row removes the backup service from the output service.
				The only way to define a higher priority backup service is to delete
				the lower priority backup services, add the new backup services and
				add the lower priority backup services again."
			::= { dcmServiceBackupServicesEntry 6 }

		
		dcmTsBackup OBJECT IDENTIFIER ::= { dcmMuxSettings 9 }

		
--  Input ts backup settings
		dcmTsBackupSettingsTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmTsBackupSettingsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with settings for ts backup."
			::= { dcmTsBackup 1 }

		
		dcmTsBackupSettingsEntry OBJECT-TYPE
			SYNTAX DcmTsBackupSettingsEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of dcmTsBackupSettingsTable"
			INDEX { dcmTsBuSettingsTsIndex }
			::= { dcmTsBackupSettingsTable 1 }

		
		DcmTsBackupSettingsEntry ::=
			SEQUENCE { 
				dcmTsBuSettingsTsIndex
					Integer32,
				dcmTsBuSettingsBuTsIndex
					Integer32,
				dcmTsBuSettingsBehavior
					INTEGER,
				dcmTsBuSettingsToBackupDelay
					Integer32,
				dcmTsBuSettingsToMainDelay
					Integer32,
				dcmTsBuSettingsState
					INTEGER,
				dcmTsBuSettingsRowStatus
					RowStatus
			 }

		dcmTsBuSettingsTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the input transport stream that needs 
				to be backed up."
			::= { dcmTsBackupSettingsEntry 1 }

		
		dcmTsBuSettingsBuTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Identification of the backup transport stream."
			::= { dcmTsBackupSettingsEntry 2 }

		
		dcmTsBuSettingsBehavior OBJECT-TYPE
			SYNTAX INTEGER
				{
				nonRevertive(1),
				revertive(2),
        floating(3)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Behavior of the backup:
				- non revertive: when switched to backup ts, 
				                 manual switch is needed to return to main ts.
				- revertive: when switched to backup ts, 
				             return to main ts when it is present.
        - floating: when switched to other active ts,
 	            return to original active ts when one is present and 
 	            the current active ts is not present."
			::= { dcmTsBackupSettingsEntry 3 }

		
		dcmTsBuSettingsToBackupDelay OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The delay in seconds before switching to backup when
				the main ts is not present anymore."
			::= { dcmTsBackupSettingsEntry 4 }

		
		dcmTsBuSettingsToMainDelay OBJECT-TYPE
			SYNTAX Integer32 (0..2147483647)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The delay in seconds before switching back to main ts when:
				- revertive: main ts becomes present."
			::= { dcmTsBackupSettingsEntry 5 }

		
		dcmTsBuSettingsState OBJECT-TYPE
			SYNTAX INTEGER
				{
				main(1),
				backup(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Read out the current state and select a new backup state."
			::= { dcmTsBackupSettingsEntry 6 }

		
		dcmTsBuSettingsRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Adding a row defines a backup transport stream for 
				a main transport stream.
				Deleting a row indicates that the transport stream will not 
				be backed up"
			::= { dcmTsBackupSettingsEntry 7 }

		
