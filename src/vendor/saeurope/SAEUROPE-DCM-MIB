		dcmIpgwSystem OBJECT IDENTIFIER ::= { dcmIpgwSettings 7 }

		
		dcmIpgwSystemGenlockEnableTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmIpgwSystemGenlockEnableEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			::= { dcmIpgwSystem 1 }

		
		dcmIpgwSystemGenlockEnableEntry OBJECT-TYPE
			SYNTAX DcmIpgwSystemGenlockEnableEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Description."
			INDEX { dcmIpgwSystemGenlockEnableBoard, dcmIpgwSystemGenlockEnablePort, dcmIpgwSystemGenlockEnableRef }
			::= { dcmIpgwSystemGenlockEnableTable 1 }

		
		DcmIpgwSystemGenlockEnableEntry ::=
			SEQUENCE { 
				dcmIpgwSystemGenlockEnableBoard
					Integer32,
				dcmIpgwSystemGenlockEnablePort
					Integer32,
				dcmIpgwSystemGenlockEnableRef
					Integer32,
				dcmIpgwSystemGenlockEnableState
					INTEGER
			 }

		dcmIpgwSystemGenlockEnableBoard OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the board where the Genlock can be Enabled/Disabled."
			::= { dcmIpgwSystemGenlockEnableEntry 1 }

		
		dcmIpgwSystemGenlockEnablePort OBJECT-TYPE
			SYNTAX Integer32 (0)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Not Used."
			::= { dcmIpgwSystemGenlockEnableEntry 2 }

		
		dcmIpgwSystemGenlockEnableRef OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Not used.
				"
			::= { dcmIpgwSystemGenlockEnableEntry 3 }

		
		dcmIpgwSystemGenlockEnableState OBJECT-TYPE
			SYNTAX INTEGER
				{
				eActive(1),
				eNotActive(2)
				}
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Enable or disable use of Genlock for the Board"
			::= { dcmIpgwSystemGenlockEnableEntry 4 }

		
