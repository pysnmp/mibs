--  service configurations
		dcmConfigServiceTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmConfigServiceEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with the services routed from input to output. This table
				contains one row for each service present at one of the DCM's
				output ports. If the same services is passed two or more times to 
				one output transport stream only the first is shown. In that case
				it will not be possible to update the configuration for that service.
				"
			::= { dcmMuxSettings 5 }

		
		dcmConfigServiceEntry OBJECT-TYPE
			SYNTAX DcmConfigServiceEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Conceptual row of dcmConfigServiceTable"
			INDEX { dcmCfgSrvInputTsIndex, dcmCfgSrvInputServiceID, dcmCfgSrvOutputTsIndex }
			::= { dcmConfigServiceTable 1 }

		
		DcmConfigServiceEntry ::=
			SEQUENCE { 
				dcmCfgSrvInputTsIndex
					Integer32,
				dcmCfgSrvInputServiceID
					Integer32,
				dcmCfgSrvOutputTsIndex
					Integer32,
				dcmCfgSrvForceOutputServiceID
					TruthValue,
				dcmCfgSrvOutputServiceID
					Integer32,
				dcmCfgSrvForceOutputServiceName
					TruthValue,
				dcmCfgSrvOutputServiceName
					OCTET STRING,
				dcmCfgSrvRowStatus
					RowStatus,
				dcmCfgSrvState
					INTEGER
			 }

		dcmCfgSrvInputTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the stream where the service is coming in."
			::= { dcmConfigServiceEntry 1 }

		
		dcmCfgSrvInputServiceID OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The service ID (PAT programme number) of the input service."
			::= { dcmConfigServiceEntry 2 }

		
		dcmCfgSrvOutputTsIndex OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the stream where the service is going to."
			::= { dcmConfigServiceEntry 3 }

		
		dcmCfgSrvForceOutputServiceID OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Overrule the incoming service ID."
			::= { dcmConfigServiceEntry 4 }

		
		dcmCfgSrvOutputServiceID OBJECT-TYPE
			SYNTAX Integer32 (0..65535)
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Output service ID (PAT programme number) of the stream. Only settable if dcmCfgSrvForceOutputServiceID is true"
			::= { dcmConfigServiceEntry 5 }

		
		dcmCfgSrvForceOutputServiceName OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Overrule the incoming service name."
			::= { dcmConfigServiceEntry 6 }

		
		dcmCfgSrvOutputServiceName OBJECT-TYPE
			SYNTAX OCTET STRING
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Overrule the incoming service name. Only settable if dcmCfgSrvForceOutputServiceName is true. You can put here international characters and markup,
				if you use the correct DVB SI bytes (charset and so on)"
			::= { dcmConfigServiceEntry 7 }

		
		dcmCfgSrvRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Rowstatus column to create a service configuration
				Can only support one-shot creation and deletion (createAndGo(4)/destroy(6))
				SNMP UNDO of a destroy might fail.
				Current defaults for omitted columns in createAndGo: both Force flags false 
				But don't rely on these defaults.
				"
			::= { dcmConfigServiceEntry 8 }

		
		dcmCfgSrvState OBJECT-TYPE
			SYNTAX INTEGER
				{
				present(1),
				notPresent(2)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"State of the output service, based on Service-loss-at-output alarm."
			::= { dcmConfigServiceEntry 9 }

		
--  configuration of AutoPass/MPTS pass through
		dcmTsAutoPassTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmTsAutoPassEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Table with the auto pass configuration. This in an output TS feature.
				See the DCM manual for more info on the applications and limitations of this feature."
			::= { dcmMuxSettings 6 }

		
		dcmTsAutoPassEntry OBJECT-TYPE
			SYNTAX DcmTsAutoPassEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"
				Conceptual row of the dcmTsAutoPassTable.
				A number of auto pass rules can be defined for an output TS (MPTS pass through), 
				by creating a row with index dcmOutputTsIndex,dcmInputTsIndex.  These rules 
				specify from which input TS all Services, EMMs and/or unreferenced PIDs should 
				be passed to an output TS.
				Disclaimers:
				1) Only one AutoPass input TS is supported for a specific output TS.  If rows 
				are created with the same dcmOutputTsIndex, but with different dcmInputTsIndex, 
				the behaviour is undefined.
				2) If all three flags of a dcmTsAutoPassEntry are disabled, the behaviour is undefined. 
				To remove the autopass settings for dcmTsAutoPassEntry, the row should be destroyed.
				3) If dcmInputTsIndex does not exists, it is not guaranteed that an error is returned.
				"
			INDEX { dcmOutputTsIndex, dcmInputTsIndex }
			::= { dcmTsAutoPassTable 1 }

		
		DcmTsAutoPassEntry ::=
			SEQUENCE { 
				dcmTsAutoPassNewServices
					TruthValue,
				dcmTsAutoPassNewEMMs
					TruthValue,
				dcmTsAutoPassUnreferencedPIDs
					TruthValue,
				dcmTsAutoPassRowStatus
					RowStatus
			 }

		dcmTsAutoPassNewServices OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"If true, any service detected in the PAT of the Input Ts
				will be automatically passed to this OutputTs.
				If you clear this flag, any automatically passed service remains passed.
				"
			DEFVAL { true }
			::= { dcmTsAutoPassEntry 1 }

		
		dcmTsAutoPassNewEMMs OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"If true, any emm detected in the CAT of the Input Ts  
				will be automatically passed to this OutputTs.
				If you clear this flag, any automatically passed emm remains passed.
				"
			DEFVAL { true }
			::= { dcmTsAutoPassEntry 2 }

		
		dcmTsAutoPassUnreferencedPIDs OBJECT-TYPE
			SYNTAX TruthValue
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"If true, any unreferenced pid detected in the Input Ts 
				will be automatically passed to this OutputTs.
				If you clear this flag, any automatically passed unreferenced pid disappears again.
				"
			DEFVAL { true }
			::= { dcmTsAutoPassEntry 3 }

		
		dcmTsAutoPassRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Rowstatus column to enable AutoPass.
				Supports only one-shot creation and deletion (createAndGo(4)/destroy(6))
				SNMP UNDO of a destroy might fail.
				"
			::= { dcmTsAutoPassEntry 4 }

		
