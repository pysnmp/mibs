--  transrater group configurations
		dcmTrGroupTable OBJECT-TYPE
			SYNTAX SEQUENCE OF DcmTrGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"The transrater group table keeps an overview of all
				defined transrater groups per board over a DCM."
			::= { dcmTrSettings 1 }

		
		dcmTrGroupEntry OBJECT-TYPE
			SYNTAX DcmTrGroupEntry
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"A row of the table holds all the objects that characterize
				an MPEG program stream."
			INDEX { dcmTrGroupBoardNumber, dcmTrGroupPortNumber, dcmTrGroupId, dcmTrGroupOutputTs }
			::= { dcmTrGroupTable 1 }

		
		DcmTrGroupEntry ::=
			SEQUENCE { 
				dcmTrGroupBoardNumber
					Integer32,
				dcmTrGroupPortNumber
					Integer32,
				dcmTrGroupId
					Integer32,
				dcmTrGroupOutputTs
					Integer32,
				dcmTrGroupBitRate
					Integer32,
				dcmTrGroupType
					INTEGER,
				dcmTrGroupRowStatus
					RowStatus
			 }

		dcmTrGroupBoardNumber OBJECT-TYPE
			SYNTAX Integer32 (1..4)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the (output) board where the transrater group is
				associated to."
			::= { dcmTrGroupEntry 1 }

		
		dcmTrGroupPortNumber OBJECT-TYPE
			SYNTAX Integer32 (1..10)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the (output) port where the transrater group is
				associated to."
			::= { dcmTrGroupEntry 2 }

		
		dcmTrGroupId OBJECT-TYPE
			SYNTAX Integer32 (2..501)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"Identification of the transrater group given board and port."
			::= { dcmTrGroupEntry 3 }

		
		dcmTrGroupOutputTs OBJECT-TYPE
			SYNTAX Integer32 (100000000..999999999)
			MAX-ACCESS not-accessible
			STATUS current
			DESCRIPTION
				"For sPTS transrating groups, set this number to 999999999. For mPTS
				and mPTSSelectiveService groups, this is the identification of
				the output transport stream the transrater group is associated
				to."
			::= { dcmTrGroupEntry 4 }

		
		dcmTrGroupBitRate OBJECT-TYPE
			SYNTAX Integer32
			UNITS "kbit/s"
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Maximal transrater group bitrate"
			::= { dcmTrGroupEntry 5 }

		
		dcmTrGroupType OBJECT-TYPE
			SYNTAX INTEGER
				{
				sPTS(1),
				mPTS(2),
				mPTSSelectiveService(3)
				}
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"Type of transrating group. A transrater group is of one of the
				following types:
				sPTS: A transrating group containing zero or more MPEG-2 transport
				  streams. Each transport streams contains a single program
				  (service).
				mPTS: A transrating group containing exactly one MPEG-2 transport
				  stream. The transport stream contains zero or more programs
				  (services).
				mPTSSelectiveService: A transrating group that contains zero or
				  more programs (services). This group subordinates
				  to a transport stream.
				  "
			::= { dcmTrGroupEntry 6 }

		
		dcmTrGroupRowStatus OBJECT-TYPE
			SYNTAX RowStatus
			MAX-ACCESS read-create
			STATUS current
			DESCRIPTION
				"RowStatus to create a group
				Can only support one-shot creation and deletion (createAndGo(4)/destroy(6))         
				SNMP UNDO of a destroy might fail.
				dcmTrGroupType is mandatory
				Current defaults for omitted columns in createAndGo: bitrate=15Mbit
				But don't rely on these defaults.
				"
			::= { dcmTrGroupEntry 7 }

		
