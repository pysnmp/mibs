            ::= { optionKeyEntry 3 }

        
        optionKeyStatus OBJECT-TYPE
            SYNTAX INTEGER
                {
                deactivated(1),
                installed(2),
                warning(3),
                fault(4),
                empty(5)
                }
            MAX-ACCESS read-only
            STATUS current
            DESCRIPTION
                "The status of this option key. 
                deactivated(1) means, the function is locked. Typically a demo key ran out.
                installed(2) means, the function is unlocked and operable.
                warning(3) means, the option is expired.
                fault(4) means, the option is required, but not installed.
                "
            ::= { optionKeyEntry 4 }

        
