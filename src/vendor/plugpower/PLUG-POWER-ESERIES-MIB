   fcsERunLowVoltThreshold OBJECT-TYPE
       SYNTAX     DisplayString (SIZE (0..30))
       MAX-ACCESS read-write
       STATUS     current
       DESCRIPTION
               "The low voltage at which the system will start
                automatically if enabled."
       ::= { fcsERun 5 }

   fcsERunLowVoltStartMode OBJECT-TYPE
       SYNTAX     INTEGER {
            off(1),
            float(2),
            maintain(3)
       }
       MAX-ACCESS read-write
       STATUS     current
       DESCRIPTION
               "Disables low voltage start or Enables low voltage
                start in either float or maintain modes."
       ::= { fcsERun 6 }

   fcsERunLowVoltTimer OBJECT-TYPE
       SYNTAX     INTEGER {
            minutes005(5),
            minutes010(10),
            minutes015(15),
            minutes020(20),
            minutes025(25),
            minutes030(30),
            minutes035(35),
            minutes040(40),
            minutes045(45),
            minutes050(50),
            minutes055(55),
            minutes060(60),
            minutes065(65),
            minutes070(70),
            minutes075(75),
            minutes080(80),
            minutes085(85),
            minutes090(90),
            minutes095(95),
            minutes100(100),
            minutes105(105),
            minutes110(110),
            minutes115(115),
            minutes120(120),
            minutes240(240)
       }
       MAX-ACCESS read-write
       STATUS     current
       DESCRIPTION
               "Number of minutes to maintain Float Voltage on
                the batteries before shutting down.  Valid minutes
                are between 5 and 120 in 5 minute increments and
                240.  A value of 0 sets a 1/2 volt above the float
                voltage before the low volt start will end, this
                requires another voltage source to drive the 1/2
                volt above.  A value of 0 will continue to run on
                a low volt start until another voltage source picks
                up the load."
       ::= { fcsERun 7 }

   fcsERunLowVoltageFloatTimeOut OBJECT-TYPE
       SYNTAX     DisplayString (SIZE (0..30))
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               "The current time at float voltage, (mm:ss)."
       ::= { fcsERun 8 }

