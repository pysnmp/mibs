   --
   -- The Alarm group.
   --

   fcsEAlarm            OBJECT IDENTIFIER ::= { fcsEObjects 4 }

   fcsEAlarms OBJECT-TYPE
       SYNTAX     DisplayString (SIZE(0..31))
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               "The present number of Major and Minor alarms."
       ::= { fcsEAlarm 1 }

   fcsEAlarmTable OBJECT-TYPE
       SYNTAX     SEQUENCE OF FcsEAlarmEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
               "A list of alarm table entries.  The table contains
               zero, one, or many rows at any moment, depending upon
               the number of alarm conditions in effect.  The agent
               creates a row in the table each time a condition is
               detected and deletes that row when that condition no
               longer pertains.  The fcsEAlarmId that indexes the table
               is the index into the products alarm table. Consequently
               the table is sparse, e.g., containing entries
               for alarm indices 5, 10, 11, and 20 and the entries should
               not be assumed to be in chronological or alarm severity
               order.

               Alarms are named by an AutonomousType (OBJECT IDENTIFIER),
               fcsEAlarmDescr.  No two rows will have the same value of
               fcsEAlarmDescr, since alarms define conditions.  In order
               to meet this requirement, care should be taken in the
               definition of alarm conditions to insure that a system
               cannot enter the same condition multiple times simultaneously.

               The number of rows in the table at any given time is
               reflected by the value of fcsEAlarmsPresent."
       ::= { fcsEAlarm 2 }

   fcsEAlarmEntry OBJECT-TYPE
       SYNTAX     FcsEAlarmEntry
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
               "An entry containing information applicable to a
               particular alarm."
       INDEX { fcsEAlarmId }
       ::= { fcsEAlarmTable 1 }

   FcsEAlarmEntry ::= SEQUENCE {
       fcsEAlarmId        PositiveInteger,
       fcsEAlarmDescr     AutonomousType,
       fcsEAlarmType      INTEGER,
       fcsEAlarmTime      DisplayString
   }

   fcsEAlarmId OBJECT-TYPE
       SYNTAX     PositiveInteger
       MAX-ACCESS not-accessible
       STATUS     current
       DESCRIPTION
               "A unique identifier for an alarm condition.  This
               value must remain constant."
       ::= { fcsEAlarmEntry 1 }

   fcsEAlarmDescr OBJECT-TYPE
       SYNTAX     AutonomousType
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               "A reference to an alarm description object.  The
               object referenced should not be accessible, but rather
               be used to provide a unique description of the alarm
               condition."
       ::= { fcsEAlarmEntry 2 }

   fcsEAlarmType OBJECT-TYPE
       SYNTAX     INTEGER {
           major(1),
           minor(2)
       }
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               "The type of alarm, Major or Minor."
       ::= { fcsEAlarmEntry 3 }

   fcsEAlarmTime OBJECT-TYPE
       SYNTAX     DisplayString (SIZE (0..31))
       MAX-ACCESS read-only
       STATUS     current
       DESCRIPTION
               "The date and time of when the alarm became active
                in the format mm/dd/yy hh:mm:ss"
       ::= { fcsEAlarmEntry 4 }


--*****************************************************************************
-- Plug Power Alarm OID section below

   --
   -- alarm conditions.
   --

   fcsESystemAlarms    OBJECT IDENTIFIER ::= { fcsEObjects 5 }

   fcsEAlarmNoCurrentAlarms OBJECT-IDENTITY
       STATUS     current
       DESCRIPTION
               "No current alarms.  Not used."
       ::= { fcsESystemAlarms  0 }

   fcsMJAlarmVerMismatch OBJECT-IDENTITY
       STATUS     current
       DESCRIPTION
               "Major Alarm:  Comm Card running an incompatible
                version to the Controller running version."
       ::= { fcsESystemAlarms  1 }

   fcsMJAlarmTermVoltsLow OBJECT-IDENTITY
       STATUS     current
       DESCRIPTION
               "Major Alarm:  The System's Terminal voltage is so
                low that there isn't enough power for the fuel
                cell system to start."
       ::= { fcsESystemAlarms  2 }

