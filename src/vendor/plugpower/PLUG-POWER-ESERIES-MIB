-- Plug Power Alarm OID section above this
--*****************************************************************************


   --
   -- notifications, i.e., traps
   --
   fcsETraps              OBJECT IDENTIFIER ::= { fcsESeries 2 }

   -- This section defines the notifications sent by an E-1100 or E-2500
   -- Care must be taken to insure that no particular notification
   -- is sent to a single receiving entity more often than once
   -- every five seconds.

   fcsETrapMJAlarmEntryAdded NOTIFICATION-TYPE
       OBJECTS { fcsEAlarmDescr }
       STATUS  current
       DESCRIPTION
               "This trap is sent each time a major alarm is inserted
               into the alarm table."
     ::= { fcsETraps 1 }

   fcsETrapMJAlarmEntryRemoved NOTIFICATION-TYPE
       OBJECTS { fcsEAlarmDescr }
       STATUS  current
       DESCRIPTION
               "This trap is sent each time a major alarm is removed
               from the alarm table."
     ::= { fcsETraps 2 }

   fcsETrapMNAlarmEntryAdded NOTIFICATION-TYPE
       OBJECTS { fcsEAlarmDescr }
       STATUS  current
       DESCRIPTION
               "This trap is sent each time a minor alarm is inserted
               into the alarm table."
     ::= { fcsETraps 3 }

   fcsETrapMNAlarmEntryRemoved NOTIFICATION-TYPE
       OBJECTS { fcsEAlarmDescr }
       STATUS  current
       DESCRIPTION
               "This trap is sent each time a minor alarm is removed
               from the alarm table."
     ::= { fcsETraps 4 }

   fcsETrapRunning NOTIFICATION-TYPE
       OBJECTS { fcsEMainDate, fcsEMainTime }
       STATUS  current
       DESCRIPTION
               "The Fuel Cell System is running.
                This trap gets sent when the system goes into
                run mode.  The fcsETrapRunEnded trap will
                be sent when the system exits run mode."
     ::= { fcsETraps 5 }

   fcsETrapExercising NOTIFICATION-TYPE
       OBJECTS { fcsEMainDate, fcsEMainTime }
       STATUS  current
       DESCRIPTION
               "The Fuel Cell System is exercising.
                This trap gets sent when the system goes into
                exercise mode.  The fcsETrapRunEnded trap will
                be sent when the system exits exercise mode."
     ::= { fcsETraps 6 }

   fcsETrapRunEnded NOTIFICATION-TYPE
       OBJECTS { fcsEMainDate, fcsEMainTime }
       STATUS  current
       DESCRIPTION
               "The Fuel Cell System just shut down from either
                exercising or running."
     ::= { fcsETraps 7 }

   fcsETrapLowH2FuelAlertOn NOTIFICATION-TYPE
       OBJECTS { fcsEMainDate, fcsEMainTime }
       STATUS  current
       DESCRIPTION
               "Low H2 Fuel alert is on."
     ::= { fcsETraps 8 }

   fcsETrapLowH2FuelAlertOff NOTIFICATION-TYPE
       OBJECTS { fcsEMainDate, fcsEMainTime }
       STATUS  current
       DESCRIPTION
               "Low H2 Fuel alert is off."
     ::= { fcsETraps 9 }

   END

