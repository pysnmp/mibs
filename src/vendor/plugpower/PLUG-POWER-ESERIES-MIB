    PLUG-POWER-ESERIES-MIB DEFINITIONS ::= BEGIN

    --
    -- Plug Power ESeries Mib
    -- Version 01.01.02
    -- Plug Power Part Number: 142-111863
    --
    -- Note:    The SNMP OID fcsEMainMIBVersion010102
    --          below has the ending 010102 which represents
    --          this version and the returned ascii string from
    --          the system should be "01.01.02".  If the
    --          fcsEMainMIBVersion010102 OID does not get a
    --          returned ascii string of "01.01.02" then this
    --          MIB is not compatible with the system.
    --
    -- Note:    The initial MIB, version 01.00.00 was released in
    --          the documentation system as version 01.00.01.
    --          So the first changed version is 01.00.02.
    --
    -- Changes in 01.00.02
    --          Added fcsEMainName
    --          Added fcsEMainLocation
    --          Added fcsEExerciseTimeOfDay
    --
    -- Changes in 01.00.03
    --          Added fcsEMainResetSystem
    --          Added fcsERunPowerLimiting
    --          Added fcsERunClearNoPressureAlarm
    --          Added fcsMJAlarmModsTooHotToRun
    --
    -- Changes in 01.00.04
    --          Removed fcsEExerciseReserved1StartStop
    --          Removed fcsEExerciseReserved2StartStop
    --
    -- Changes in 01.00.05
    --          Added fcsERunLowH2PressureAlarm
    --                fcsERunAuxOutAssigned
    --                fcsERunSystemLockTime
    --                fcsERunSystemLockTimeOut
    --                fcsERunLockSystem
    --                fcsEExerciseCond1StartStop
    --                fcsMJAlarmSystemLocked
    --                fcsMJAlarmTesting
    --                fcsMNAlarmTesting
    --                fcsMNAlarmLowH2Pressure
    --
    -- Changes in 01.00.06
    --          Changed fcsERunLowVoltTimer to show
    --          half-volt-above-float for a value of 0 and
    --          minutesxxx where xxx is the numerical value
    --          of minutes to set for everything else.
    --          Changed fcsERunPowerLimiting display from
    --          xx-percent to percentxxx so the selection
    --          would show up in increasing order.  It use
    --          to list 100 percent first then 20 percent.
    --
    -- Changes in 01.01.00
    --          Removed half-volt-above-float setting from
    --          fcsERunLowVoltTimer
    --          Added Exercise to fcsERunAuxOutAssigned
    --          Added fcsEMainVolts
    --                fcsEMainAmps
    --                fcsEMainWatts
    --                fcsERunAuxInAssigned
    --                fcsERunVolts
    --                fcsERunAmps
    --                fcsERunWatts
    --
    -- Changes in 01.01.01
    --          Changed descriptions for fcsETrapRunning
    --          and fcsETrapExercising to specify that
    --          fcsETrapRunEnded will be sent when system
    --          shuts down instead of fcsETrapShutDown.
    -- Changes in 01.01.02
    --          Added fcsMNAlarmDoorOpen
    --                fcsMNAlarmSystemDateInvalid


    IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
        OBJECT-IDENTITY, Integer32, enterprises
            FROM SNMPv2-SMI
        DisplayString, AutonomousType, TEXTUAL-CONVENTION
            FROM SNMPv2-TC
        MODULE-COMPLIANCE, OBJECT-GROUP, NOTIFICATION-GROUP
            FROM SNMPv2-CONF;

    -- org         OBJECT IDENTIFIER ::= {iso 3}
    -- dod         OBJECT IDENTIFIER ::= {org 6}
    -- internet    OBJECT IDENTIFIER ::= {dod 1}
    -- private     OBJECT IDENTIFIER ::= {internet 4}
    -- enterprises OBJECT IDENTIFIER ::= {private 1}
    -- plugPowerMIB OBJECT IDENTIFIER ::= {enterprises 17022}

    plugPowerMIB MODULE-IDENTITY
       LAST-UPDATED "201603030000Z" -- March 3,2016
       ORGANIZATION "Plug Power, Inc"
       CONTACT-INFO
              "
                Postal: Plug Power
                        15913 East Euclid Avenue
                        Spokane, Wa  99216

                   Tel: +1 509-228-6500
       24 Hour Support: +1 866-661-0020

              Internet: www.plugpower.com
                E-mail: GenSureTechSupport@plugpower.com"

       DESCRIPTION
               "MIB module for Plug Power products."
       REVISION "201603030000Z" -- March 3,2016
       DESCRIPTION
               "Seventh version."
       -- 1.3.6.1.4.1.17022 --
       ::= {enterprises 17022}

   PositiveInteger ::= TEXTUAL-CONVENTION
       DISPLAY-HINT "d"
       STATUS       current
       DESCRIPTION
               "This data type is a non-zero and non-negative value."
       SYNTAX       Integer32 (1..2147483647)

   NonNegativeInteger ::= TEXTUAL-CONVENTION
       DISPLAY-HINT "d"
       STATUS       current
       DESCRIPTION
               "This data type is a non-negative value."
       SYNTAX       Integer32 (0..2147483647)

    FloatingPointNumber ::= TEXTUAL-CONVENTION
       DISPLAY-HINT "x"
       STATUS       current
       DESCRIPTION
               "This data type is a floating point number."
       SYNTAX       Integer32 (0..2147483647)

