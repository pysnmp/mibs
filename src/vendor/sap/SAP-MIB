SAP-MIB DEFINITIONS ::= BEGIN

  IMPORTS
	MODULE-IDENTITY, OBJECT-TYPE, NOTIFICATION-TYPE,
	Counter32, Gauge32, IpAddress
		FROM SNMPv2-SMI
	DisplayString
		FROM SNMPv2-TC
	 enterprises
               FROM RFC1155-SMI;

  sap OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"The SAP enterprise MIB tree."
	::= { enterprises 694 }

  r3    MODULE-IDENTITY
	LAST-UPDATED "9505110655Z"
	ORGANIZATION "SAP AG
	 	      Neurottstrasse 16
		      69190 Walldorf Germany"
	CONTACT-INFO "Guenter Zachmann"
	DESCRIPTION
		"The MIB module to describe objects for the SAP R/3
		 system."
	::= { sap 1 }

  r3Objects OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"The r3Objects tree is further organized into four groups;
		 system, config, alert and performance"
	::= { r3 1 }


  r3system 	OBJECT IDENTIFIER ::= { r3Objects 1 }
  r3config 	OBJECT IDENTIFIER ::= { r3Objects 2 }
  r3alert  	OBJECT IDENTIFIER ::= { r3Objects 3 }
  r3perf   	OBJECT IDENTIFIER ::= { r3Objects 4 }
  r3maAlert OBJECT IDENTIFIER ::= { r3Objects 5 }
  r3maiAlert OBJECT IDENTIFIER ::= { r3Objects 6 }
  r3customAlert OBJECT IDENTIFIER ::= { r3Objects 7 }

-- ==========================================================================
-- The R/3 System Group
-- ==========================================================================

  r3systemTable OBJECT-TYPE
	SYNTAX     SEQUENCE OF R3systemEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"The (conceptual) table of active SAP R/3
		 systems. If a system is configured but not
	 	 started, it will not be in the list."
	::= { r3system 1 }

  r3systemEntry OBJECT-TYPE
	SYNTAX     R3systemEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A (conceptual) entry for one active SAP R/3
		 system. As an example, an instance of the
		 r3systemName object might be named
		 r3systemName.3"
	INDEX { r3systemNumber }
	::= { r3systemTable 1 }

  R3systemEntry ::= SEQUENCE {
		       r3systemNumber        INTEGER,
		       r3systemReleaseString DisplayString,
		       r3systemName          DisplayString,
		       r3systemLogonString   DisplayString
		    }

  r3systemNumber OBJECT-TYPE
	SYNTAX	   INTEGER (0..2147483647)
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"The SAP system number. Management requests
		 to currently inactive SAP R/3 systems will
		 fail."
	::= { r3systemEntry 1 }

  r3systemReleaseString OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..20))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Represents the SAP R/3 Release"
	::= { r3systemEntry 2 }

  r3systemName	OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..20))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"The SAP system name for the SAP system
		 specified by the SAP R/3 system number.
		 In addition to the system number
		 each system can have a system name"
	::= { r3systemEntry 3 }

  r3systemLogonString	OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..256))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"The logon string for the SAP R/3 system specified
		 by the SAP R/3 system number. On a UNIX system,
		 this string can be directly executed."
	::= { r3systemEntry 4 }

--
-- r3systemTraps
--

  r3systemTraps OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		""
	::= { r3system 2 }

  r3trapInstanceStartUp NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"Inform the Network Management System if a new
		 R/3 instance is coming up."
	::= { r3systemTraps 2 }

  r3trapInstanceShutDown NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"Inform the Network Management System if a
		 R/3 instance is going down."
	::= { r3systemTraps 3 }


-- #==========================================================================
-- # The R/3 Config Group
-- #==========================================================================

  r3configTable OBJECT-TYPE
	SYNTAX     SEQUENCE OF R3configEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A list of R/3 Configuration entries"
	::= { r3config 1 }

  r3configEntry OBJECT-TYPE
	SYNTAX     R3configEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A (conceptual) entry for one configuration
		information data set. The r3systemNumber in the
		index represents the entry in the r3systemTable
		that corresponds to the r3configEntry."
	INDEX { r3systemNumber }
	::= { r3configTable 1 }

  R3configEntry ::= SEQUENCE {
		       r3configSystemNumber INTEGER,
		       r3configInstanceName DisplayString,
		       r3configDBName       DisplayString,
		       r3configDBHostName   DisplayString,
		       r3configDBHostIpAddr IpAddress
		    }

  r3configSystemNumber OBJECT-TYPE
	SYNTAX	   INTEGER (0..2147483647)
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"The r3configSystemNumber corresponds to the
		first index r3systemNumber."
	::= { r3configEntry 1 }

  r3configInstanceName OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"This MIB variable contains the name of the
		R/3-Instance"
	::= { r3configEntry 2 }

  r3configDBName	OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"This MIB variable contains the
		 SAP R/3 Database Name"
	::= { r3configEntry 3 }

  r3configDBHostName OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"This MIB variable contains the host name of
		 the SAP R/3 database server"
	::= { r3configEntry 4 }

  r3configDBHostIpAddr OBJECT-TYPE
	SYNTAX	   IpAddress
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"This MIB variable contains the IP address of the host
		where the SAP R/3 database is located. May also be
		the IP address for connecting the R3 database."
	::= { r3configEntry 5 }

  r3configServiceTable OBJECT-TYPE
	SYNTAX     SEQUENCE OF R3configServiceEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A list of R/3-Service-Types for each R/3-Instance"
	::= { r3config 2 }


  r3configServiceEntry OBJECT-TYPE
	SYNTAX     R3configServiceEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A (conceptual) entry for one service
		information data set. The r3systemNumber in the
		index represents the entry in the r3systemTable
		that corresponds to the r3configEntry."
	INDEX { r3systemNumber, r3configServiceType }
	::= { r3configServiceTable 1 }

  R3configServiceEntry ::= SEQUENCE {
                              r3configServiceSystemNumber INTEGER,
                              r3configServiceType         INTEGER
                           }

  r3configServiceSystemNumber OBJECT-TYPE
	SYNTAX	    INTEGER (0..2147483647)
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The r3configServiceSystemNumber corresponds
		to the first index r3systemNumber."
	::= { r3configServiceEntry 1 }

  r3configServiceType OBJECT-TYPE
	SYNTAX  INTEGER {
                    dialogService           (1),
                    updateService           (2),
                    enqueueService          (3),
                    backgroundService       (4),
                    messageService          (5),
                    gatewayService          (6),
                    spoolService            (7)
                }
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"This MIB variable contains one Service offered
		by the corresponding R/3-Instance"
	::= { r3configServiceEntry 2 }

--
-- r3configTraps
--

  r3configTraps OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		""
	::= { r3config 3 }

  r3trapStateChange NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"Services of the instance have changed."
	::= { r3configTraps 1 }


-- ==========================================================================
-- The R/3 Alert Group
-- ==========================================================================

  r3alertTable OBJECT-TYPE
	SYNTAX     SEQUENCE OF R3alertEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"A list of R/3 Alert entries "
	::= { r3alert 1 }

  r3alertEntry OBJECT-TYPE
	SYNTAX     R3alertEntry
	MAX-ACCESS not-accessible
	STATUS     current
	DESCRIPTION
		"The remaining variables are needed for
		the trap facility. When a SAP system
		reports an alert condition, the agent
		will send a SNMP trap to the registered
		management station.
		The Trap PDU contains the MIB variables defined below.
		The r3systemNumber in the index represents the entry
		in the r3systemTable that corresponds to the
		r3alertEntry"
	INDEX { r3systemNumber , r3alertClass }
	::= { r3alertTable 1 }

  R3alertEntry ::= SEQUENCE {
		      r3alertSystemNumber   INTEGER,
		      r3alertClass          INTEGER,
		      r3alertNumber         INTEGER,
		      r3alertAgentHostName  DisplayString,
		      r3alertSapNameString  DisplayString,
		      r3alertSeverity       INTEGER,
		      r3alertMsgString      DisplayString,
		      r3alertActionString   DisplayString,
		      r3alertTimeStamp      Counter32
		   }
				
  r3alertSystemNumber OBJECT-TYPE
	SYNTAX	   INTEGER (0..2147483647)
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"The r3alertSystemNumber corresponds
		to the first index r3systemNumber."
	::= { r3alertEntry 1 }

  r3alertClass	OBJECT-TYPE
	SYNTAX	   INTEGER {
                      classStateChange  (1),
                      classSAPsysUp     (2),
                      classSAPsysDown   (3),
                      classSlogId       (10),
                      classSlogFreq     (11),
                      classBuf          (12),
                      classEnqueue      (13),
                      classRollpag      (14),
                      classTrace        (15),
                      classDpQueue      (16),
                      classPerfDia      (20),
                      classPerfUpd      (21),
                      classPerfBtc      (22),
                      classPerfSpo      (23),
                      classAbapUpd      (30),
                      classAbapErr      (31),
                      classAbapSql      (32),
                      classDbIndcs      (41),
                      classDbFreSp      (42),
                      classDbArcSt      (43),
                      classDbBckup      (44),
                      classSpo          (51),
                      classArch         (52),
                      classGenP3        (53),
                      classGenP4        (54),
                      classGenP5        (55),
                      classGenP6        (56),
                      classGenP7        (57),
                      classGenP8        (58)
                   }
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
        "Identifies the R3 Alert classes

         Class name         -  Comment
         -----------------------------------------------------------------
         classNull             not available

                               indicating state change if an instance
         classStateChange      services of an instance have changed
         classSAPsysUp         R3 instance coming up
         classSAPsysDown       R3 instance shut down

         classSlogId           critical systemlog occurred
         classSlogFreq         frequent writes into systemlog

         classBuf              local buffers alert, directory or storage
                               shortage, bad efficiency

         classEnqueue          lock facility alert, directory or storage
                               shortage, errors occurred

         classRollpag          rollfile, paging file shortage

         classTrace            trace on, may cause reduced performance

         classDpQueue          shortage in the request queues of an
                               application server

                               performance alert, exceeding wait time or
                               response time, exceeding processing time of
                               ABAP-programs
         classPerfDia          in dialog task
         classPerfUpd          in update task
         classPerfBtc          in background task
         classPerfSpo          in spool task

                               alerts in ABAP processor
         classAbapUpd          error in update processing
         classAbapErr          other error
         classAbapSql          error during database access

                               database alerts
         classDbIndcs          missing indices
         classDbFreSp          free space shortage
         classDbArcSt          archiver stuck
         classDbBckup          backup failed

                               ABAP-programmable general purpose alerts
                               GENP0..1 already used for spool and archive

         classSpo              spooler subsystem alert, exxeding time or
                               spool database shortage

         classArch             archive subsystem alert, archive-link

                               ABAP-programmable general purpose alerts
         classGenP3            reserved for further use
         classGenP4            reserved for further use
         classGenP5            reserved for further use
         classGenP6            reserved for further use
         classGenP7            reserved for further use
         classGenP8            reserved for further use"
	::= { r3alertEntry 2 }

  r3alertNumber	OBJECT-TYPE
	SYNTAX	   INTEGER (0..2147483647)
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Represents the alert number"
	::= { r3alertEntry 3 }

  r3alertAgentHostName OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..100))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Represents the agent host name"
	::= { r3alertEntry 4 }

  r3alertSapNameString OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..120))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Represents the SAP R/3 server name string."
	::= { r3alertEntry 5 }

  r3alertSeverity	OBJECT-TYPE
	SYNTAX     INTEGER {
  		      severityNormal   (1),
  		      severityWarning  (2),
  		      severityCritical (3)
  		   }
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION
        "Identifies the R3 Alert severity

         Severity           -  Comment
         -----------------------------------------------------------------
         severityNull          not available
         severityNormal        normal operation state, no problems
         severityWarning       problems coming up
         everityCritical       severe problems
                               for some alerts the severity is derived
                               from configurable high water marks or
                               thresholds"
	::= { r3alertEntry 6 }

  r3alertMsgString        OBJECT-TYPE
	SYNTAX     DisplayString (SIZE (0..256))
	MAX-ACCESS read-only
	STATUS     current
	DESCRIPTION
		"Represents the alert message string"
	::= { r3alertEntry 7 }

  r3alertActionString 	OBJECT-TYPE
	SYNTAX	   DisplayString (SIZE (0..256))
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Represents the alert action string"
	::= { r3alertEntry 8 }

  r3alertTimeStamp	OBJECT-TYPE
	SYNTAX	   Counter32
	MAX-ACCESS read-only
	STATUS	   current
	DESCRIPTION
		"Time (UTC) of Alert Occurrance
		 UTC = time in seconds since 1970, Greenwich"
	::= { r3alertEntry 9 }

--
-- r3alertTraps
--

  r3alertTraps OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		""
	::= { r3alert 2 }

  r3trapSyslogIdNormal NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"An entry in the systemlog was written, which was configured
		 critical in the systemlog-filter. Normal Severity."
	::= { r3alertTraps 30 }

  r3trapSyslogIdWarning NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"An entry in the systemlog was written, which was configured
		 critical in the systemlog-filter. Warning Severity."
	::= { r3alertTraps 31 }

  r3trapSyslogIdCritical NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"An entry in the systemlog was written, which was configured
		 critical in the systemlog-filter. Critical Severity"
	::= { r3alertTraps 32 }

  r3trapSyslogFreqNormal NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"A lot of entrys in the systemlog. The reason can be, that the
		 defined rate of entries (modulo) or the entries per hour went
		 beyond the defined limit. Normal Severity."
	::= { r3alertTraps 33 }

  r3trapSyslogFreqWarning NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"A lot of entrys in the systemlog. The reason can be, that the
		 defined rate of entries (modulo) or the entries per hour went
		 beyond the defined limit. Warning Severity."
	::= { r3alertTraps 34 }

  r3trapSyslogFreqCritical NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"A lot of entrys in the systemlog. The reason can be, that the
		 defined rate of entries (modulo) or the entries per hour went
		 beyond the defined limit. Critical Severity"
	::= { r3alertTraps 35 }

  r3trapBufferSumNormal	NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"The bufferperformance (hit/access) goes beyond a limiting value
		 or the size of the buffer-directory or the buffer-memory had
		 passed a defined value. Normal Severity."
	::= { r3alertTraps 36 }

  r3trapBufferSumWarning	NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
		"The bufferperformance (hit/access) goes beyond a limiting value
		 or the size of the buffer-directory or the buffer-memory had
		 passed a defined value. Warning Severity."
	::= { r3alertTraps 37 }

  r3trapBufferSumCritical	NOTIFICATION-TYPE
	OBJECTS	{
