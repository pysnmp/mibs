-- #==========================================================================
-- # SAP R/3 Conformance
-- #==========================================================================

  r3Conformance OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		"The SAP-MIB conformance statements"
	::= { r3 2 }

  r3Compliances OBJECT IDENTIFIER ::= { r3Conformance 1 }
  r3Groups      OBJECT IDENTIFIER ::= { r3Conformance 2 }

  r3Compliance MODULE-COMPLIANCE
	STATUS current
	DESCRIPTION
		""
	MODULE SAP-MIB
		MANDATORY-GROUPS { r3ObjectsGroup }
	::= { r3Compliances 1 }

  r3ObjectsGroup OBJECT-GROUP
	OBJECTS {
	   r3systemNumber, r3systemReleaseString, r3systemName,
	   r3systemLogonString,
	   r3configSystemNumber, r3configInstanceName, r3configDBName,
	   r3configDBHostName, r3configDBHostIpAddr,
	   r3configServiceSystemNumber, r3configServiceType,
	   r3alertSystemNumber, r3alertClass, r3alertNumber,
	   r3alertAgentHostName, r3alertSapNameString, r3alertSeverity,
	   r3alertMsgString, r3alertActionString, r3alertTimeStamp,
	   r3perfSystemNumber, r3perfServiceType, r3perfEventsFrequency,
	   r3perfResponseTime, r3perfWaitTime
	}
	STATUS current
	DESCRIPTION
		""
	::= { r3Groups 1 }

-- #==========================================================================
-- # SAP R/3 4.X Alert Traps
-- #==========================================================================

  r3maAlertTable OBJECT-TYPE
    SYNTAX     SEQUENCE OF R3maAlertEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
           "A list of R/3 monitoring architecture alert entries "
    ::= { r3maAlert  1 }


  r3maAlertEntry OBJECT-TYPE
    SYNTAX     R3maAlertEntry
    MAX-ACCESS not-accessible
    STATUS     current
    DESCRIPTION
           "The remaining variables are needed for
            the trap facility. When a SAP system
            reports an alert condition, the agent
            will send a SNMP trap to the registered
            management station.
            The Trap PDU contains the MIB variables defined below.
	    The table-entries are the alerts which occured.
            An alert is identified by a r3maAlertSystemName, r3maAlertSegmentName,
            r3maAlertUniqnum, r3maAlertIndex, r3maAlertDate, r3maAlertDate.
            It is possible to acknowledge an alert with this information."
    INDEX   {r3maAlertSystemName, r3maAlertSegmentName, r3maAlertUniqnum, r3maAlertIndex,r3maAlertDate,r3maAlertTime }
    ::= { r3maAlertTable  1 }


  R3maAlertEntry ::= SEQUENCE {
		      r3maAlertSystemName   DisplayString,
		      r3maAlertSegmentName  DisplayString,
		      r3maAlertUniqnum      DisplayString,
		      r3maAlertIndex        DisplayString,
		      r3maAlertDate         DisplayString,
		      r3maAlertTime         DisplayString,
		      r3maAlertContextName  DisplayString,
		      r3maAlertObjectName   DisplayString,
	       	  r3maAlertAttribName   DisplayString,
		      r3maAlertValue	    INTEGER,
		      r3maAlertSeverity     INTEGER,
		      r3maAlertMsgString    DisplayString
    }


  r3maAlertSystemName OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..8))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the System that raised the alert"
    ::= { r3maAlertEntry  1 }

  r3maAlertSegmentName OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..40))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the segment name that raised the alert"
    ::= { r3maAlertEntry  2 }


  r3maAlertUniqnum OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..10))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the number of the alert.
            An alert is identified by a r3maAlertSystemName, r3maAlertSegmentName,
            r3maAlertUniqnum, r3maAlertIndex, r3maAlertDate, r3maAlertDate.
            It is possible to acknowledge an alert with this information."
    ::= { r3maAlertEntry  3 }

  r3maAlertIndex OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..10))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the index of the alert.
            An alert is identified by a r3maAlertSystemName, r3maAlertSegmentName,
            r3maAlertUniqnum, r3maAlertIndex, r3maAlertDate, r3maAlertDate.
            It is possible to acknowledge an alert with this information."
    ::= { r3maAlertEntry  4 }


  r3maAlertDate OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..8))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the date of alert occurrance.
            An alert is identified by a r3maAlertSystemName, r3maAlertSegmentName,
            r3maAlertUniqnum, r3maAlertIndex, r3maAlertDate, r3maAlertDate.
            It is possible to acknowledge an alert with this information."
    ::= { r3maAlertEntry  5 }


  r3maAlertTime OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..6))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the time of alert occurrance.
            An alert is identified by a r3maAlertSystemName, r3maAlertSegmentName,
            r3maAlertUniqnum, r3maAlertIndex, r3maAlertDate, r3maAlertDate.
            It is possible to acknowledge an alert with this information."
    ::= { r3maAlertEntry  6 }



  r3maAlertContextName OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..40))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the context name in a monitoring segment."
    ::= { r3maAlertEntry  7 }

  r3maAlertObjectName OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..40))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the monitor object name."
    ::= { r3maAlertEntry  8 }

  r3maAlertAttribName OBJECT-TYPE
    SYNTAX     DisplayString(SIZE (0..40))
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the monitor attribute name."
    ::= { r3maAlertEntry  9 }


  r3maAlertValue OBJECT-TYPE
    SYNTAX  INTEGER {
        valueNormal(1),
    	valueWarning(2),
    	valueCritical(3),
    	valueUnknown(4)
  }
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Identifies the R3 Alert Value

         Value           -  Comment
         -----------------------------------------------------------------
         valueNormal           normal operation state, green
         valueWarning          warning, yellow
         valueCritical         problem occurred, red
	 valueUnknown	       unknown status"
    ::= { r3maAlertEntry  10 }


  r3maAlertSeverity OBJECT-TYPE
    SYNTAX    INTEGER (0..255)
    MAX-ACCESS read-only
    STATUS    current
    DESCRIPTION
           "Identifies the R3 alert severity as configured in RZ20"
    ::= { r3maAlertEntry  11 }

  r3maAlertMsgString OBJECT-TYPE
    SYNTAX     DisplayString
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
           "Represents the alert message string"
    ::= { r3maAlertEntry  12 }



  r3maAlertTraps OBJECT IDENTIFIER ::= { r3maAlert  2 }

  r3maTrap NOTIFICATION-TYPE
    OBJECTS   {
      r3maAlertSystemName,
      r3maAlertSegmentName,
      r3maAlertUniqnum,
      r3maAlertIndex,
      r3maAlertDate,
      r3maAlertTime,
      r3maAlertContextName,
      r3maAlertObjectName,
      r3maAlertAttribName,
      r3maAlertValue,
      r3maAlertSeverity,
      r3maAlertMsgString
    }
    STATUS current    
    DESCRIPTION
           "Generic SAP R/3 4.X Alert"
    ::= { r3maAlertTraps 60 }

	
