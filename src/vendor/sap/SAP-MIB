  r3perfTraps OBJECT-IDENTITY
	STATUS current
	DESCRIPTION
		""
	::= { r3perf 2 }

  r3trapPerfDiaNormal NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
	   r3alertActionString,
	   r3alertTimeStamp
	}
	STATUS current
	DESCRIPTION
	    "You will have performance-traps, when the waiting time, the
             response time or the operating time of an ABAP-Program has passed
             a defined value. Work-Process-Typ Dialog: Performance goes
             beneath a limit. Normal Severity."
	::= { r3perfTraps 60 }

