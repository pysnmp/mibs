  r3trapDpQueueNormal NOTIFICATION-TYPE
	OBJECTS	{
	   r3systemNumber,
	   r3alertNumber,
	   r3alertAgentHostName,
	   r3alertSapNameString,
	   r3alertClass,
	   r3alertSeverity,
	   r3alertMsgString,
